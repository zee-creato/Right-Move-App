{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "cresprep": "replace-in-files --string='type=\\\"module\\\"' --replacement='defer' dist/app/index.html",
    "cresarc": "ch5-cli archive -p app -d dist/app -o dist  && ch5-cli deploy -H 192.168.0.126 -t touchscreen dist/app.ch5z  ",
    "cresbuild": "npm run build && npm run cresprep && npm run cresarc",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "archive": "ch5-cli archive -p app -d dist/app -o dist && ch5-cli deploy -H 192.168.0.126 -t touchscreen dist/app.ch5z"
  },
  "private": true,
